{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DisCatSharp": {
    "Using": [
      "DisCatSharp.Interactivity",
      "CommandsNext"
    ],

    "Discord": {
      "Token": "",
      "UseCanary": true,
      "AutoReconnect": true,
      "MobileStatus":  true
    },

    "Interactivity": {
      "PollBehaviour": "KeepEmojis"
    },

    "CommandsNext": {
      "StringPrefixes": [ "dcs!" ],
      "CaseSensitive": true,
      "IgnoreExtraArguments":  true
    }
  }
}
